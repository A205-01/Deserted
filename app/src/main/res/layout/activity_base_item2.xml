<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="@color/listvb_bg"
    android:id="@+id/list_bg">
    <TextView
        android:text="eee"
        android:id="@+id/base_item_name2"
        android:layout_width="wrap_content"
        android:layout_marginLeft="38dp"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:layout_marginStart="38dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <ImageView
        android:background="@drawable/finish"
        android:id="@+id/base_item_done2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:gravity="center_vertical"
        android:layout_above="@+id/textView4"
        android:layout_centerHorizontal="true" />
    <TextView
        android:text="eee"
        android:id="@+id/base_item_time2"
        android:layout_toLeftOf="@id/base_item_done"
        android:layout_marginTop="20dp"
        android:layout_marginRight="10dp"
        android:layout_width="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="eee"
        android:id="@+id/textView2"
        android:layout_alignTop="@+id/textView6"
        android:layout_alignLeft="@+id/base_item_name2"
        android:layout_alignStart="@+id/base_item_name2" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="eee"
        android:layout_alignTop="@+id/textView2"
        android:layout_toRightOf="@+id/textView2"
        android:layout_toEndOf="@+id/textView2"
        android:id="@+id/textView3"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="eee"
        android:id="@+id/textView4"
        android:layout_alignTop="@+id/textView3"
        android:layout_centerHorizontal="true" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:text="eee"
        android:layout_alignTop="@+id/textView4"
        android:layout_toRightOf="@+id/textView4"
        android:layout_toEndOf="@+id/textView4" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/gp"
        android:id="@+id/textView5"
        android:layout_alignTop="@+id/textView6"
        android:layout_alignRight="@+id/base_item_time2"
        android:layout_alignEnd="@+id/base_item_time2" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:text="eee"
        android:layout_below="@+id/base_item_time2"
        android:layout_toLeftOf="@+id/textView5"
        android:layout_toStartOf="@+id/textView5" />
</RelativeLayout>
